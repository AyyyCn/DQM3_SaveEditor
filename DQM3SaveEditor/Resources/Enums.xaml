<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <x:Array x:Key="Seasons" Type="system:String">
        <system:String>Spring</system:String>
        <system:String>Summer</system:String>
        <system:String>Autumn</system:String>
        <system:String>Winter</system:String>
    </x:Array>

    <x:Array x:Key="Weathers" Type="system:String">
        <system:String>Fine</system:String>
        <system:String>Rain</system:String>
        <system:String>HeatRain</system:String>
        <system:String>Snow</system:String>
        <system:String>Storm</system:String>
    </x:Array>
</ResourceDictionary> 